{
  "name": "stroller_roller_backend",
  "version": "1.0.0",
  "description": "NodeJs API for MongoDB",
  "scripts": {
    "start": "node ./start.js",
    "start-dev": "nodemon ./start.js",
    "code:check": "yarn code:lint; yarn code:format --check",
    "code:clean": "yarn code:lint --fix; yarn code:format --write",
    "code:lint": "eslint --ext .js,.jsx,.ts,.tsx \"src/\"",
    "code:format": "prettier './src/**/**.{js,jsx,ts,tsx}'",
    "code:check:rules": "eslint --print-config .eslintrc.js | eslint-config-prettier-check"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/treuna/stroller_roller.git"
  },
  "author": "treuna",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/treuna/stroller_roller/issues"
  },
  "homepage": "https://github.com/treuna/stroller_roller#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "esm": "^3.2.25",
    "express": "^4.17.3",
    "express-jwt": "^6.0.0",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.5.2",
    "mongoose": "^5.8.11",
    "rootpath": "^0.1.2"
  },
  "devDependencies": {
    "nodemon": "^2.0.2",
    "poetic": "^1.4.0"
  }
}
